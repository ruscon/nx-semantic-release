    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "eslint": "8.46.0",
    "eslint-config-prettier": "8.6.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.6.0",
    "nx": "16.7.4",
    "prettier": "^2.8.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.0.0"
  }
